   "source": [
    "num = int(input(\"Enter a number:\"))\n",
    "total= 0\n",
    "\n",
    "if num % 2 == 0:\n",
    "    for i in range(1,num+1):\n",
    "        if i % 3 == 0:\n",
    "            total += i\n",
    "else:\n",
    "    for i in range(1,num+1):\n",
    "        if i % 5 == 0:\n",
    "            total += i\n",
    "\n",
    "result = total*len(str(num))\n",
    "print(\"Result=\",result)"
   ]
  }
